{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": ["airbnb"],
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"react/destructuring-assignment": ["<enabled>", "always"],
		"react/display-name": [
			"<enabled>",
			{
				"ignoreTranspilerName": " <boolean>"
			}
		],
		"react/prefer-es6-class": ["<enabled>", "<mode>"],
		"react/no-unsafe": [
			"<enabled>",
			{
				"checkAliases": " <boolean>"
			}
		],
		"react/no-unknown-property": [
			"<enabled>",
			{
				"ignore": " <ignore>"
			}
		],
		"react/no-unescaped-entities": [
			"<enabled>",
			{
				"forbid": "Array<string>"
			}
		],
		"react/prop-types": [
			"<enabled>",
			{
				"ignore": " <ignore>",
				"customValidators": "<customValidator>",
				"skipUndeclared": "<skipUndeclared>"
			}
		]
	}
}
