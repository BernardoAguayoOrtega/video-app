{
	"env": {
		"browser": true,
		"es2020": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended", "airbnb"],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 11
	},
	"plugins": ["react"],
	"rules": {
		"react/destructuring-assignment": ["<enabled>", "always"],
		"react/display-name": [
			"<enabled>",
			{
				"ignoreTranspilerName": " <boolean>"
			}
		],
		"react/prefer-es6-class": ["<enabled>", "<mode>"],
		"react/no-unsafe": [
			"<enabled>",
			{
				"checkAliases": " <boolean>"
			}
		],
		"react/no-unknown-property": ["<enabled>", { "ignore": " <ignore>" }],
		"react/no-unescaped-entities": [
			"<enabled>",
			{
				"forbid": "Array<string>"
			}
		],
		"react/prop-types": [
			"<enabled>",
			{
				"ignore": " <ignore>",
				"customValidators": "<customValidator>",
				"skipUndeclared": "<skipUndeclared>"
			}
		]
	}
}
